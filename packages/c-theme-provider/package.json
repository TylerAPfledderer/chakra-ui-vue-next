{
  "name": "@chakra-ui/c-theme-provider",
  "version": "2.0.0-beta.1",
  "main": "dist/chakra-ui-c-theme-provider.cjs.js",
  "module": "dist/chakra-ui-c-theme-provider.esm.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/chakra-ui-c-theme-provider.cjs.js",
      "default": "./dist/chakra-ui-c-theme-provider.esm.js"
    }
  },
  "description": "Chakra UI Vue | CThemeProvider component",
  "repository": "https://github.com/chakra-ui/chakra-ui-vue-next/tree/master/packages/c-theme-provider",
  "author": "Jonathan Bakebwa codebender828@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build": "tsup && pnpm build:types",
    "build:fast": "tsup",
    "types:check": "tsc --noEmit",
    "dev": "tsup --watch",
    "build:types": "tsup src --dts-only",
    "build:rollup": "rollup -c && pnpm build:types"
  },
  "dependencies": {
    "@chakra-ui/theme": "^2.2.5",
    "@chakra-ui/theme-tools": "2.0.17"
  },
  "devDependencies": {
    "@chakra-ui/theme": "^2.2.5",
    "@chakra-ui/theme-tools": "2.0.17",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "name": "chakra-ui-c-theme-provider",
    "clean": true,
    "target": "es2019",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": {
      "chakra-ui-c-theme-provider": "src/index.tsx"
    },
    "keepNames": true
  }
}