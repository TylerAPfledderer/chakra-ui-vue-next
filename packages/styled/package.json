{
  "name": "@chakra-ui/vue-styled",
  "version": "2.1.0-beta.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "description": "Chakra UI Vue | Styled module",
