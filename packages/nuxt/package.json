{
  "name": "@chakra-ui/nuxt-next",
  "version": "2.0.0-beta.1",
  "description": "Nuxt 3 compatible version of Chakra UI Vue",
  "main": "dist/chakra-ui-nuxt-next.cjs.js",
  "module": "dist/chakra-ui-nuxt-next.esm.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/chakra-ui-nuxt-next.cjs.js",
      "default": "./dist/chakra-ui-nuxt-next.esm.js"
    }
  },
  "repository": "https://github.com/chakra-ui/chakra-ui-vue-next/tree/master/packages/nuxt",
  "author": "Jonathan Bakebwa codebender828@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build": "nuxt-module-build",
    "build:fast": "nuxt-module-build",
    "dev:build": "nuxi build playground",
    "dev": "nuxi dev playground",
    "types:check": "tsc --noEmit",
    "build:types": "tsup src --dts-only"
  },
  "dependencies": {
    "@chakra-ui/vue-next": "workspace:*",
    "@emotion/css": "^11.10.5",
    "@emotion/server": "^11.10.0"
  },
  "peerDependencies": {
    "vue": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.0.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxt/test-utils": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "nuxt": "^3.0.0",
    "vue": "^3.2.37"
  }
}
